<application>
<name>POETICON++ World State from Dummy File</name>

<module>
    <name>worldStateManager</name>
    <parameters>--playback dbdummy.ini</parameters>
    <node>console</node>
</module>
<module>
    <name>objectsPropertiesCollector</name>
    <parameters>--name memory --context poeticon --db dbhands.ini --nosave</parameters>
    <node>console</node>
</module>
<module>
    <name>opc2prada</name>
    <node>console</node>
</module>

<connection>
    <from>/wsm/opc:io</from>
    <to>/memory/rpc</to>
    <protocol>tcp</protocol>
</connection>
<connection>
    <from>/opc2prada/rpcClient</from>
    <to>/memory/rpc</to>
    <protocol>tcp</protocol>
</connection>
<connection>
    <from>/memory/broadcast:o</from>
    <to>/opc2prada/broadcast:i</to>
    <protocol>tcp</protocol>
</connection>
<connection>
    <from>/opc2prada/cmd:io</from>
    <to>/planner/cmd:io</to>
    <protocol>tcp</protocol>
</connection>
<connection>
    <from>/planner/cmd:io</from>
    <to>/opc2prada/cmd:io</to>
    <protocol>tcp</protocol>
</connection>

</application>
